{
  "expo": {
    "name": "SeuChef",
    "slug": "seu-chef",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC143C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.nocton.seuchef",
      "versionCode": 1
    },
    "androidStatusBar": {
      "backgroundColor": "#DC143C",
      "barStyle": "light-content",
      "translucent": false
    }
  }
}
